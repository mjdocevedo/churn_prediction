name: churn-prediction

python_env: pyproject.toml

entry_points:
  main:
    command: "python src/train.py"
  evaluate:
    parameters:
      model_uri: {type: uri}
      data_path: {type: string, default: "data/telco_churn.csv"}
    command: "python src/evaluate.py --model_uri {model_uri} --data_path {data_path}"
